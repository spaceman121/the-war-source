sound dlgaudio;
void dlgplay(string dlgsound, bool dlgfade=false, int fadespeed=30, bool is_file=true)
{
if(is_file)
dlgaudio.load(dlgsound);
else
dlgaudio.load_from_memory(dlgsound);
dlgaudio.play();
while(dlgaudio.playing)
{
wait(5);
if(key_pressed(KEY_RETURN))
{
if (dlgfade==true)
{
}
dlgaudio.stop();
break;
}
}
}