void ammo_give(string we, int amount)
{
int a;
if (!ammo.exists(we))
{
ammo.set(we,amount);
}
else
{
ammo.get(we,a);
if (a+amount<=0)
ammo.delete(we);
else
{
int b=(a+amount);
ammo.delete(we);
ammo.set(we,b);
}
}
}
